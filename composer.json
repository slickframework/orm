{
  "name": "Slick/orm",
  "description": "Slick Object-relational mapping package",
  "minimum-stability": "dev",
  "license": "MIT",
  "version": "v1.2.0",
  "keywords": [
    "slick",
    "ORM",
    "Object-relational mapping"
  ],
  "authors": [
    {
      "name": "Filipe Silva",
      "email": "silvam.filipe@gmail.com",
      "role": "Developer"
    }
